    "version": "0.2.0",
    "configurations": [

        {
            "name": "Launch server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/server/.env",
            "program": "${workspaceFolder}/server/work/cmd/server/main.go"
        },

        {
            "name": "Launch Oauth emulator",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/oauth-emulator/.env",
            "program": "${workspaceFolder}/oauth-emulator/main.go"
        }
    ]
}